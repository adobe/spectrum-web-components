{
    "name": "@spectrum-web-components/color-field",
    "version": "0.42.2",
    "publishConfig": {
        "access": "public"
    },
    "description": "Web component implementation of a Spectrum design ColorField",
    "license": "Apache-2.0",
    "repository": {
