{
    "name": "@lliad-ui/react",
    "version": "0.0.1",
    "publishConfig": {
        "access": "public"
    },
    "description": "Web component implementation of a Spectrum design React",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/adobe/spectrum-web-components.git",
        "directory": "packages/react"
    },
    "author": "",
    "homepage": "https://adobe.github.io/spectrum-web-components/components/react",
    "bugs": {
        "url": "https://github.com/adobe/spectrum-web-components/issues"
    },
    "main": "src/index.js",
    "module": "src/index.js",
    "type": "module",
    "exports": {
        ".": "./src/index.js",
        "./src/*": "./src/*.js",
        "./package.json": "./package.json",
        "./sp-react": "./sp-react.js",
        "./sp-react.js": "./sp-react.js"
    },
    "scripts": {
        "test": "echo \"Error: run tests from mono-repo root.\" && exit 1"
    },
    "customElementsManifest": "custom-elements.json",
    "files": [
        "**/*.d.ts",
        "**/*.js",
        "**/*.js.map",
        "!test/",
        "!stories/",
        "custom-elements.json"
    ],
    "keywords": [
        "spectrum css",
        "web components",
        "lit-element",
        "lit-html"
    ],
    "dependencies": {
        "@lliad-ui/base": "^0.7.2",
        "@lit-labs/react": "^1.0.1",
        "react": "^17.0.2",
        "tslib": "^2.0.0"
    },
    "types": "./src/index.d.ts",
    "sideEffects": [
        "./sp-*.js"
    ]
}
