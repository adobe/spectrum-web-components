        "@spectrum-web-components/icons-workflow": "^0.47.0",
        "@spectrum-web-components/link": "^0.47.0",
        "@spectrum-web-components/menu": "^0.47.0"
    },
    "devDependencies": {
        "@spectrum-css/breadcrumb": "^9.1.3"
    },
    "types": "./src/index.d.ts",
    "customElements": "custom-elements.json",
    "sideEffects": [
        "./sp-*.js",
        "./**/*.dev.js"
    ]
