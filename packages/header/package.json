    "exports": {
        ".": {
            "development": "./src/index.dev.js",
            "default": "./src/index.js"
        },
        "./package.json": "./package.json",
        "./src/Header.js": {
            "development": "./src/Header.dev.js",
            "default": "./src/Header.js"
        },
        "./src/header-overrides.css.js": "./src/header-overrides.css.js",
        "./src/header.css.js": "./src/header.css.js",
        "./src/spectrum-header.css.js": "./src/spectrum-header.css.js",
        "./src/index.js": {
            "development": "./src/index.dev.js",
            "default": "./src/index.js"
        },
        "./sp-header.js": {
            "development": "./sp-header.dev.js",
            "default": "./sp-header.js"
        }
    },
    "scripts": {
        "test": "karma start --coverage"
    },
    "files": [
        "**/*.d.ts",
        "**/*.js",
        "**/*.js.map",
        "custom-elements.json",
        "!stories/",
        "!test/"
