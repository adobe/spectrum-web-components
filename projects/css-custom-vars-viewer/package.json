{
    "name": "@spectrum-web-components/custom-vars-viewer",
    "version": "1.6.0",
    "description": "Demo app created with Spectrum Web Components, following open-wc guidelines",
    "license": "Apache-2.0",
    "author": "Adobe",
    "main": "dist/src/index.js",
    "module": "dist/src/index.js",
    "private": true,
    "type": "module",
    "exports": {
        ".": "./dist/src/index.js",
        "./sp-css-table.js": "./dist/src/sp-css-table.js",
        "./custom-vars-viewer.js": "./dist/src/custom-vars-viewer.js"
    },
    "engines": {
        "node": ">=16.14.2",
        "yarn": ">=1.16.0"
    },
    "scripts": {
        "analyze": "cem analyze --litelement",
        "build": "yarn parse-json && tsc && npm run analyze -- --exclude dist",
        "format": "eslint --ext .ts,.html . --fix --ignore-path .gitignore && prettier \"**/*.ts\" --write --ignore-path .gitignore",
        "lint": "eslint --ext .ts,.html . --ignore-path .gitignore && prettier \"**/*.ts\" --check --ignore-path .gitignore",
        "parse-json": "node ./scripts/parse-json.js",
        "prepublish": "tsc && npm run analyze -- --exclude dist",
        "test": "tsc && wtr --coverage",
        "test:watch": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wtr --watch\""
    },
    "dependencies": {
        "@rollup/plugin-json": "^6.1.0",
        "@spectrum-web-components/base": "1.6.0",
        "@spectrum-web-components/field-label": "1.6.0",
        "@spectrum-web-components/link": "1.6.0",
        "@spectrum-web-components/menu": "1.6.0",
        "@spectrum-web-components/picker": "1.6.0",
        "@spectrum-web-components/story-decorator": "^1.6.0",
        "@spectrum-web-components/styles": "1.6.0",
        "@spectrum-web-components/swatch": "1.6.0",
        "@spectrum-web-components/table": "1.6.0",
        "@spectrum-web-components/theme": "1.6.0",
        "@spectrum-web-components/toast": "1.6.0"
    },
    "devDependencies": {
        "@custom-elements-manifest/analyzer": "^0.10.4",
        "@open-wc/eslint-config": "^12.0.3",
        "@open-wc/testing": "^4.0.0",
        "@typescript-eslint/eslint-plugin": "^8.33.0",
        "@typescript-eslint/parser": "^7.18.0",
        "@web/test-runner": "^0.18.3",
        "concurrently": "^8.2.2",
        "tslib": "^2.8.1",
        "typescript": "^5.8.3"
    },
    "customElements": "custom-elements.json",
    "repository": {
        "type": "git",
        "url": "https://github.com/adobe/spectrum-web-components.git",
        "directory": "projects/css-custom-vars-viewer"
    },
    "bugs": {
        "url": "https://github.com/adobe/spectrum-web-components/issues"
    },
    "keywords": [
        "design-system",
        "spectrum",
        "adobe",
        "adobe-spectrum",
        "web components",
        "web-components",
        "lit-element",
        "lit-html"
    ],
    "homepage": "https://opensource.adobe.com/spectrum-web-components/"
}
