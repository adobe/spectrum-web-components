        "postcss-prefixwrap": "^1.22.1",
        "postcss-preset-env": "^6.7.0",
        "postcss-reporter": "^7.0.2",
        "postcss-selector-parser": "^6.0.4",
        "posthtml": "^0.16.4",
        "posthtml-attrs-parser": "^0.1.1",
        "posthtml-loader": "^1.0.2",
        "posthtml-match-helper": "^1.0.1",
        "prettier": "^2.2.1",
        "prettier-plugin-package": "^1.3.0",
        "pretty-bytes": "^5.6.0",
        "pretty-quick": "^3.1.0",
        "prismjs": "^1.15.0",
        "raw-loader": "^4.0.0",
        "re-template-tag": "^2.0.1",
        "rimraf": "^3.0.0",
        "serve-index": "^1.9.1",
        "sinon": "^11.1.1",
        "style-loader": "^2.0.0",
        "stylelint": "^13.13.0",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-standard": "^22.0.0",
        "tachometer": "^0.5.8",
        "ts-loader": "^8.0.0",
        "typescript": "^4.3.2",
        "walker": "^1.0.7",
        "webpack": "^4.41.6",
        "webpack-bundle-analyzer": "^4.3.0",
        "webpack-cli": "^4.3.1",
        "webpack-dev-server": "^3.10.3",
        "webpack-merge": "^4.2.2",
        "webpackbar": "^4.0.0",
        "yargs": "^17.0.1"
    },
    "resolutions": {
        "cssnano/**/postcss-calc": "7.0.0",
        "playwright": "1.13.0"
    },
    "customElements": "documentation/custom-elements.json",
    "workspaces": [
        "packages/*",
        "projects/*"
    ]
}
