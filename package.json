{
    "name": "@crisp/spectrum-web-components",
    "publishConfig": {
        "registry": "https://artifactory.corp.adobe.com/artifactory/api/npm/npm-crisp-release-local/"
    },
    "version": "0.0.6",
    "description": "",
    "files": [
        "lib",
        "styles"
    ],
    "sideEffects": [
        "./lib/*/index.js",
        "./styles/*.css"
    ],
    "main": "lib/index.js",
    "module": "lib/index.js",
    "browser": "lib/index.js",
    "engines": {
        "node": ">=10.0.0",
        "npm": ">=6.4.0"
    },
    "scripts": {
        "split-spectrum": "node ./scripts/split-spectrum.js",
        "process-spectrum": "node ./scripts/process-spectrum-css.js; npx pretty-quick --pattern 'src/**/*.css'",
        "process-icons": "node ./scripts/process-icons.js",
        "prebuild": "rimraf lib",
        "build": "gulp",
        "watch": "gulp watch",
        "serve": "node ./scripts/serve.js",
        "serve:watch": "npm run build && (npm run serve & npm run watch)",
        "storybook": "start-storybook -s styles -p 6006",
        "lint": "bash ./scripts/lint.sh",
        "test": "npm run build && karma start --single-run",
        "test:nobuild": "karma start",
        "test:nobuildsingle": "karma start --single-run",
        "test:watch": "npm run build && (npm run watch & karma start)"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged && npm run lint"
        }
    },
