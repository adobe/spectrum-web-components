{
    "name": "@crisp/spectrum-web-components",
    "publishConfig": {
        "registry": "https://artifactory.corp.adobe.com/artifactory/api/npm/npm-crisp-release-local/"
    },
    "version": "0.0.7",
    "description": "",
    "files": [
        "lib",
        "styles"
    ],
    "sideEffects": [
        "./lib/*/index.js",
        "./src/*/index.ts",
        "./styles/*.css",
        "./documentation/src/components/*.ts"
    ],
    "main": "lib/index.js",
    "module": "lib/index.js",
    "browser": "lib/index.js",
    "engines": {
        "node": ">=10.0.0",
        "npm": ">=6.4.0"
    },
    "scripts": {
        "spectrum-vars": "node ./scripts/spectrum-vars.js",
        "process-spectrum": "node ./scripts/process-spectrum-css.js; npx pretty-quick --pattern 'packages/**/*.css'",
        "process-icons": "node ./scripts/process-icons.js",
        "build": "gulp",
        "build:tests": "tsc --build test/tsconfig.json && tsc --build test/tsconfig-node.json",
        "watch": "gulp watch",
        "serve": "node ./scripts/serve.js",
        "serve:watch": "npm run build && (npm run serve & npm run watch)",
        "storybook": "start-storybook -s styles,stories/assets -p 6006",
        "docs:build": "gulp docsCompile",
        "docs:start": "gulp docsWatchCompile",
        "lint": "bash ./scripts/lint.sh",
        "test": "npm run test:build && karma start --coverage",
        "test:build": "npm run build && tsc --build test/tsconfig-test.json",
        "test:watch": "npm run watch & npm run test:build -- -w & karma start --auto-watch=true --single-run=false",
        "test:update-snapshots": "npm run test:build && karma start --update-snapshots",
        "test:prune-snapshots": "npm run test:build && karma start --prune-snapshots",
        "test:compatibility": "npm run test:build && karma start --compatibility all --auto-watch=true --single-run=false",
        "test:bench": "node test/benchmark/cli.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged && npm run lint"
        }
    },
    "peerDependencies": {
        "common-tags": "^1.8.0",
        "lit-element": "^2.1.0",
        "lit-html": "^1.0.0",
        "prismjs": "^1.15.0"
    },
