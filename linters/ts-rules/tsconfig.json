{
    "compilerOptions": {
        "allowJs": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "react",
        "lib": ["es6", "dom"],
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "outDir": ".",
        "rootDir": ".",
        "skipLibCheck": false,
        "sourceMap": false,
        "strictNullChecks": true,
        "suppressImplicitAnyIndexErrors": true,
        "target": "es5"
    },
    "exclude": ["nothing"],
    "include": ["*.ts"]
}
