{
    "files.exclude": {
        "**/*.css.ts": { "when": "$(basename)" },
        "**/*.js.map": true,
        "**/*.js": { "when": "$(basename).ts" },
        "**/*.dev.js": { "when": "$(basename).js" },
        "**/*.d.ts": { "when": "$(basename).ts" },
        "**/*.test-vrt.ts": true
    },
    "typescript.tsdk": "node_modules/typescript/lib",
    "lit-plugin.strict": true
}
